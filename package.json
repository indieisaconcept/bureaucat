{
  "name": "bureaucat",
  "version": "0.1.0",
  "description": "Templated transformation for JSON",
  "main": "lib/index.js",
  "scripts": {
    "clean": "trash --force ./test/coverage",
    "project": "projectz compile",
    "lint": "eslint ./lib",
    "cover": "npm run clean && istanbul cover --dir ./test/coverage _mocha ./test",
    "test": "npm run clean && npm run lint && mocha ./test",
    "coveralls": "npm run cover && cat ./test/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Jonathan Barnett <me@indieisaconcept.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/indieisaconcept/bureaucat"
  },
  "license": {
    "type": "MIT"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "coveralls": "^2.11.2",
    "eslint": "^0.19.0",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.4",
    "trash": "^1.4.1"
  },
  "bugs": {
    "url": "https://github.com/indieisaconcept/bureaucat/issues"
  },
  "homepage": "https://github.com/indieisaconcept/bureaucat",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "JSON",
    "transform",
    "normalize",
    "template",
    "schema"
  ],
  "maintainers": [
    ""
  ],
  "contributors": []
